                      (count(tokenize(tokenize($path, '\[')[1], '\.')) = 7 and 'boolean' = local-name(root()/jx:object[@name = tokenize(tokenize($path, '\[')[1], '\.')[1]]/jx:property[@name = tokenize(tokenize($path, '\[')[1], '\.')[2]]/jx:property[@name = tokenize(tokenize($path, '\[')[1], '\.')[3]]/jx:property[@name = tokenize(tokenize($path, '\[')[1], '\.')[4]]/jx:property[@name = tokenize(tokenize($path, '\[')[1], '\.')[5]]/jx:property[@name = tokenize(tokenize($path, '\[')[1], '\.')[6]]/jx:property[@name = tokenize(tokenize($path, '\[')[1], '\.')[7]]/*[number(tokenize(tokenize($path, '(\[)|(\[\])|(\])')[2], '\.')[1])])) or
