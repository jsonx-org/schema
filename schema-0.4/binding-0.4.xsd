                  (count(tokenize($path, '\.')) = 4 and 'reference' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 5 and 'reference' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/jx:property[@name = tokenize($path, '\.')[5]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 6 and 'reference' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/jx:property[@name = tokenize($path, '\.')[5]]/jx:property[@name = tokenize($path, '\.')[6]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 7 and 'reference' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/jx:property[@name = tokenize($path, '\.')[5]]/jx:property[@name = tokenize($path, '\.')[6]]/jx:property[@name = tokenize($path, '\.')[7]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 8 and 'reference' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/jx:property[@name = tokenize($path, '\.')[5]]/jx:property[@name = tokenize($path, '\.')[6]]/jx:property[@name = tokenize($path, '\.')[7]]/jx:property[@name = tokenize($path, '\.')[8]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 9 and 'reference' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/jx:property[@name = tokenize($path, '\.')[5]]/jx:property[@name = tokenize($path, '\.')[6]]/jx:property[@name = tokenize($path, '\.')[7]]/jx:property[@name = tokenize($path, '\.')[8]]/jx:property[@name = tokenize($path, '\.')[9]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])))"/>
            </xs:complexType>
          </xs:element>
          <xs:element name="boolean" maxOccurs="unbounded">
            <xs:complexType>
              <xs:all>
                <xs:element name="binding" type="jb:typeFieldBinding"/>
              </xs:all>
              <xs:attribute name="path" type="xs:string"/>
              <xs:assert test="some $path in @path satisfies
                (count(tokenize($path, '\[')) = 1 and (
                  (count(tokenize($path, '\.')) = 1 and root()/jx:boolean[@name = $path]) or
                  (count(tokenize($path, '\.')) = 2 and 'boolean' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 3 and 'boolean' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 4 and 'boolean' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 5 and 'boolean' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/jx:property[@name = tokenize($path, '\.')[5]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 6 and 'boolean' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/jx:property[@name = tokenize($path, '\.')[5]]/jx:property[@name = tokenize($path, '\.')[6]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 7 and 'boolean' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/jx:property[@name = tokenize($path, '\.')[5]]/jx:property[@name = tokenize($path, '\.')[6]]/jx:property[@name = tokenize($path, '\.')[7]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 8 and 'boolean' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/jx:property[@name = tokenize($path, '\.')[5]]/jx:property[@name = tokenize($path, '\.')[6]]/jx:property[@name = tokenize($path, '\.')[7]]/jx:property[@name = tokenize($path, '\.')[8]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type'])) or
                  (count(tokenize($path, '\.')) = 9 and 'boolean' = string(root()/jx:object[@name = tokenize($path, '\.')[1]]/jx:property[@name = tokenize($path, '\.')[2]]/jx:property[@name = tokenize($path, '\.')[3]]/jx:property[@name = tokenize($path, '\.')[4]]/jx:property[@name = tokenize($path, '\.')[5]]/jx:property[@name = tokenize($path, '\.')[6]]/jx:property[@name = tokenize($path, '\.')[7]]/jx:property[@name = tokenize($path, '\.')[8]]/jx:property[@name = tokenize($path, '\.')[9]]/@*[namespace-uri() = 'http://www.w3.org/2001/XMLSchema-instance' and local-name() = 'type']))))
                  or (count(tokenize($path, '[^\]]\[')) = 2 and (
                    count(tokenize(tokenize($path, '[^\]]\[')[2], '\[')) = 1 and (
                      (count(tokenize(tokenize($path, '\[')[1], '\.')) = 1 and 'boolean' = local-name(root()/jx:array[@name = tokenize(tokenize($path, '\[')[1], '\.')[1]]/*[number(tokenize(tokenize($path, '(\[)|(\[\])|(\])')[2], '\[')[1])])) or
                      (count(tokenize(tokenize($path, '\[')[1], '\.')) = 2 and 'boolean' = local-name(root()/jx:object[@name = tokenize(tokenize($path, '\[')[1], '\.')[1]]/jx:property[@name = tokenize(tokenize($path, '\[')[1], '\.')[2]]/*[number(tokenize(tokenize($path, '(\[)|(\[\])|(\])')[2], '\.')[1])])) or
                      (count(tokenize(tokenize($path, '\[')[1], '\.')) = 3 and 'boolean' = local-name(root()/jx:object[@name = tokenize(tokenize($path, '\[')[1], '\.')[1]]/jx:property[@name = tokenize(tokenize($path, '\[')[1], '\.')[2]]/jx:property[@name = tokenize(tokenize($path, '\[')[1], '\.')[3]]/*[number(tokenize(tokenize($path, '(\[)|(\[\])|(\])')[2], '\.')[1])])) or
